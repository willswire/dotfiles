[user]
	name = William Walker
	email = w_walker@icloud.com
[fetch]
	prune = true
[commit]
	gpgsign = true
[push]
	autoSetupRemote = true
[gpg]
	program = /usr/local/bin/gpg
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
[init]
	defaultBranch = main
